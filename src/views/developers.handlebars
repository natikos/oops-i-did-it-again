<div class='card shadow p-4'>
  <h2 class='mb-4'>Developers Revenue</h2>
  <table class='table table-striped'>
    <thead>
      <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Email</th>
        <th>Revenue ($)</th>
      </tr>
    </thead>
    <tbody>
      {{#each developers}}
        <tr>
          <td>{{this.id}}</td>
          <td>{{this.firstName}} {{this.lastName}}</td>
          <td>{{this.email}}</td>
          <td>{{formatMoney this.revenue}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>